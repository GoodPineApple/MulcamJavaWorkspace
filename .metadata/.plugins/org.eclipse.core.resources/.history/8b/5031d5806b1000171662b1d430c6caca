package pascal;

public class Pascal {

	public static void main(String[] args) {
		System.out.println("1");
		System.out.println(1 + " " + 1);
		
		int[] a = new int[8];
		a[0] = 1;
		a[1] = 1;
		
		for(int i=1; i<8; i++){
			
			
			for(int j=0; j<a.length; j++){
				System.out.print(a[j]);
			}
			System.out.println();
		}
	}

}
