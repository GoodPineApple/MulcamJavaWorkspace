package pascal;

public class Pascal {

	public static void main(String[] args) {
		
		int[] a = new int[12];
		a[0] = 1;
		a[1] = 1;
		
		for(int i=1; i<10; i++){
			
			
			if(a.length>3){
				for(int x=1; x<i; x++){
					a[i-x] = a[i-1-x] + a[i-x];
				}				
			}
			
			for(int j=0; j<a.length; j++){
				a[i] = 0;
				if(a[j] != 0){
					System.out.print(a[j] + " ");
				}
			}
			System.out.println();
		}
	}

}
