package aiMatrix;

public class AiMatrix {
	public static void main(String[] args) {
		double[][] six = 
		{{1, 5, 3, 4, 6, 2},
         {7, 3, 2, 1, 3, 8},
         {8, 3, 9, 6, 1, 0},
         {5, 1, 0, 8, 3, 0},
         {0, 9, 7, 4, 3, 2},
         {2, 9, 1, 0, 7, 3}};
		double[][] five = new double[5][5];
		
		for (int i = 0; i < six.length-1; i++) {
			double a = six[i][i];
			double b = six[i][i+1];
			double c = six[i+1][i];
			double d = six[i+1][i+1];
			
			double result = (a + b + c + d) / 4;
			
			five[i][i] = result; 
			
		}
		float a = 15/4;
		System.out.println(five[4][4] + ", " + a);
	}


}
